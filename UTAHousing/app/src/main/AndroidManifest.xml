<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utahousing.seproject.com.utahousing" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".HousingLogin"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HousingHome"
            android:label="@string/title_activity_housing_home" >
        </activity>
        <activity
            android:name=".HousingApplicationHome"
            android:label="@string/title_activity_housing_application_home"
            android:parentActivityName=".HousingHome" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="utahousing.seproject.com.utahousing.HousingHome" />
        </activity>
    </application>

</manifest>
